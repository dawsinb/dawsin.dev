    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/seedrandom": "^3.0.4",
    "@types/styled-components": "^5.1.26",
    "@types/three": "^0.149.0",

    "@react-three/drei": "^9.56.25",
    "@react-three/fiber": "^8.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "seedrandom": "^3.0.5",
    "styled-components": "^5.3.6",
    "three": "^0.149.0",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
